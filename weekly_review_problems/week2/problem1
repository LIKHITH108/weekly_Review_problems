nums = [10, 20, 30, 40, 50, 60, 70, 80, 90]
index = 2  # Start at the third element (0-based index)

while nums:
    # Remove and print the element at the index
    print(nums.pop(index))
    
    # If the list is empty, break
    if not nums:
        break
    
    # Calculate the next index
    index = (index + 2) % len(nums)